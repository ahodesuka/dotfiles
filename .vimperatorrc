highlight CmdLine background: #1b1b1b; color: #e9e6e6; transition: all 0.25s;
highlight CmdLine>* font-family: "ProFont";
highlight CompDesc width: 500px; max-width: 500px; color: gray; font-family: "ProFont";
highlight CompItem[selected] background: #222;
highlight CompResult width: 500px; max-width: 500px; overflow: hidden; font-family: "ProFont";
highlight ContentSeparator border-top: 1px solid #161616; display: -moz-box;
highlight ErrorMsg color: white; background: #8f5b5b; font-weight: bold;
highlight InfoMsg color: #c98dad; background: #1b1b1b;
highlight ModeMsg color: #222; background: #9ebf7c; border-radius: 1px; padding: 3px 5px 0;
highlight MoreMsg color: #9ebf7c; background: #1b1b1b;
highlight NonText color: grey; min-height: 16px; padding-left: 2px; font-family: "ProFont";
highlight Normal color: #e9e6e6; background: #1b1b1b;
highlight PromptText color: #222; background: #c98dad; padding-top: 3px;
highlight StatusLine color: #a49c9c; background: transparent; font-weight: normal;
highlight WarningMsg color: #cf7275; background: #1b1b1b;

noremap <C-a> <Insert><C-a><Insert>
inoremap <C-a> <C-Home><C-S-End>
nnoremap > <C-i>
nnoremap < <C-o>
nnoremap <C-f> /
inoremap <C-f> <Esc>/
nnoremap <C-h> <C-p>
nnoremap <C-l> <C-Tab>
nnoremap <S-h> :tabm -1<CR>
nnoremap <S-l> :tabm +1<CR>
nnoremap <silent> <C-n> :nohl<Return>
nnoremap h <Left>
nnoremap j <Down>
nnoremap k <Up>
nnoremap l <Right>

set cpt=lf
set gui=none,tabs

" vim: set ft=vim:
